    game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(message)
        local chat = message.Message
        local speaker = message.FromSpeaker
        local prefix = _G.Prefix
        if string.find(chat, prefix.."pred ") then
            if table.find(adminusers, speaker) then
                local split = chat:split(" ")
                getgenv().Pred = split[2]
                print(getgenv().Pred)
            end
        elseif string.find(chat, prefix.."rejoin") then
            if table.find(adminusers, speaker) then
                game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId,game.JobId)
            end
        elseif string.find(chat, prefix.._G.BCMD) then
            if table.find(adminusers, speaker) then
                local split = chat:split(" ")
                plr = tostring(split[2])
                foundplr = findplr(plr)
                target = game.Players:FindFirstChild(foundplr)
                if target.Character.BodyEffects["K.O"].Value == false and target.Character.BodyEffects.Dead.Value == false then
                    SilentAimTarg = target
                    local savedpos = game.Players:FindFirstChild(speaker).Character:WaitForChild("HumanoidRootPart").Position
                    repeat wait()
