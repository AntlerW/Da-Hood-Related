        for i,v in ipairs(game:GetService("Players"):GetChildren()) do
            if (string.lower(name) == string.sub(string.lower(v.Name), 1, #name)) or (string.lower(name) == string.sub(string.lower(v.DisplayName), 1, #name)) then
